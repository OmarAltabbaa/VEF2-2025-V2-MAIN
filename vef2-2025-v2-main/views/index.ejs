<!DOCTYPE html>
<html lang="en">
<head>
    <title>Quiz Categories</title>
</head>
<body>
    <h1>Quiz Categories</h1>
    <ul>
        <% categories.forEach(category => { %>
            <li><a href="/category/<%= category.id %>"><%= category.name %></a></li>
        <% }) %>
    </ul>
    <a href="/add-question">Add Question</a>
</body>
</html>
